{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/inngest",
            "destination": "backend/src/server.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "backend/src/server.js"
        }
    ]
}